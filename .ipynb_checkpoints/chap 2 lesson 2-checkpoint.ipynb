    "from math import exp\n",
    "S = 100 #stock price\n",
    "K = 75 #strike price\n",
    "r = 0.05 #risk-free interest rate\n",
    "T = 1 #time to expiration (in years)\n",
    "d1 = 0.6 #implied volatility\n",
    "d2 = 0.5 #implied volatility\n",
    "\n",
    "print((S*(d1) - (K*exp(-r*T)*(d2))))"
   ]
